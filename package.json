{
  "name": "@liou666/eslint-config-monorepo",
  "version": "1.0.1",
  "description": "Liou's Eslint Config Rules",
  "author": "Liou (https://github.com/liou666)",
  "license": "MIT",
  "keywords": [
    "eslint-config"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=14",
    "pnpm": ">=7.0.0"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "publish": "pnpm publish  -r --access public",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag&&pnpm publish  -r --access public"
  },
  "dependencies": {
    "@liou666/eslint-config": "*",
    "@liou666/eslint-config-ts": "*",
    "eslint": "^8.25.0",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.25.0",
    "bumpp": "^8.2.1"
  }
}
